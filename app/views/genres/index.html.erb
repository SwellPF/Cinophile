<h1>List of Genres</h1>
<table class="table">
    <thead>
        <tr><th colspan="2"></th></tr>
    </thead>
    <tbody>
    <% @genre.each do |genre| %>
    <tr>
        <td>
    <%= genre.name %> </td><td><%= link_to "Edit", genre_path(genre), class: "button" %></td>
</tr>
    <% end %>
</tbody>
</table>  
<%= link_to "Add a new Genre", new_genre_path, class: "button" %>